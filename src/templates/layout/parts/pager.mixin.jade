mixin pager(from, to, current, link)
    ul.pagination
        li.disabled
            a(href='#!')
                i.material-icons chevron_left
        li.active
            a(href='#!') 1
        li.waves-effect
            a(href='#!') 2
        li.waves-effect
            a(href='#!') 3
        li.waves-effect
            a(href='#!') 4
        li.waves-effect
            a(href='#!') 5
        li.waves-effect
            a(href='#!')
                i.material-icons chevron_right
